<decoder name="sophos-v21-av">
  <prematch>^device="\w*"\s+date=\d+-\d+-\d+\s+time=\d+:\d+:\d+\s+timezone=|device_name="(\.*)"\s+timestamp="(\.*)"\s+device_model="(\.*)"\s+device_serial_id="(\.*)"\s+log_id=|^messageid="\d+"\s+log_type="Event"\s+log_component="\.*"\s+log_subtype="\w+"\s+|^client_used="\.*"\s+auth_mechanism="\.*"\s+status="\.*"|^reason="\.*"\s+client_used="\.*"\s+auth_mechanism="\.*"\s+s</prematch>
</decoder>

